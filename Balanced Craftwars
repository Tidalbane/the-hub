local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Tidal Hub") -- Creates the window

local Player = game.Players.LocalPlayer

local b = w:CreateFolder("Hacks") -- Creates the folder(U will put here your buttons,etc)
local c = w:CreateFolder("Settings")

getgenv().EggAutopickup = false

--[[b:Label("ESP",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

c:Label("Settings",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) ]]

c:DestroyGui()

function Autopickup()
    spawn(function()
        while getgenv().EggAutopickup == true do
            wait(3)
            for _,PotentialEgg in pairs(workspace:GetChildren()) do
                if PotentialEgg:IsA('MeshPart') and PotentialEgg.Name == 'Egg' then
                    PotentialEgg.CFrame = CFrame.new(Player.Character.HumanoidRootPart.Position)
                end
            end
        end
    end)
end

--[[b:Button("Button",function()
    print("Elym Winning")
end)]]

b:Toggle("Egg Autopickup",function(bool)
    getgenv().EggAutopickup = bool
    Autopickup()
end)




