function CollectorNotifier()
    spawn(function()
        while getgenv().CollectorNotifier == true do
            Silver.Text = game.Players.LocalPlayer.Data.BellProgression.Value
            Shadow.Text = game.Players.LocalPlayer.Data.BellProgression.Value
            local NPCFolder = workspace.NPCs.Misc
            if NPCFolder.DesertCollector.Head.Transparency ~= 1 then
                Desert.TextColor3 = Color3.fromRGB(0,255,0)
            else
                Desert.TextColor3 = Color3.fromRGB(85,0 ,0)
            end
            if NPCFolder.ForestCollector.Head.Transparency ~= 1 then
                Forest.TextColor3 = Color3.fromRGB(0,255,0)
            else
                Forest.TextColor3 = Color3.fromRGB(85,0 ,0)
            end
            if NPCFolder.TundraCollector.Head.Transparency ~= 1 then
                Tundra.TextColor3 = Color3.fromRGB(0,255,0)
            else
                Tundra.TextColor3 = Color3.fromRGB(85,0 ,0)
            end
            if NPCFolder.ScroomvilleCollector.Head.Transparency ~= 1 then
                Scroom.TextColor3 = Color3.fromRGB(0,255,0)
            else
                Scroom.TextColor3 = Color3.fromRGB(85,0 ,0)
            end
            wait(1)
        end
    end)
end


c:Toggle("Collector Notifier",function(bool)
    getgenv().CollectorNotifier = bool
    if bool == true then
        collector.Enabled = bool
        CollectorNotifier()
    else
        collector.Enabled = bool
    end
end)

e:Toggle("No Kill Bricks",function(bool)
    getgenv().NoKillbrick = bool
    for _,Child in pairs(workspace.Map.KillBricks:GetChildren()) do
        if Child:IsA('Part') then
            if bool then
                Child.CanTouch = false
            elseif not bool then
                Child.CanTouch = true
            end
        end
    end
end)

e:Toggle("No Fall Damage",function(bool)
    getgenv().Nofall = bool
    local Character = game.Players.LocalPlayer.Character
    local Script = Character.FallDamage:WaitForChild()

    if bool then
        Script.Disabled = true
    else
        Script.Disabled = false
    end
end)


e:Button("Fullbright",function()
    loadstring(game:HttpGet("https://pastebin.com/raw/06iG6YkU", true))()
end)

e:Button("Infinite Yield",function()
